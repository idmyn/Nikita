<p id="notice"><%= notice %></p>

<div class="container">
  <div class="row justify-content-center">
    <div class="col-6">

      <h1 class="text-center">Users</h1><br>

    </div>
  </div>
</div>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


